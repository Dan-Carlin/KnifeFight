{
  "expo": {
    "name": "KnifeFight",
    "slug": "KnifeFight",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/kf_icon_logo.png",
    "splash": {
      "image": "./app/assets/logos_splash_screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./app/assets/icons/kf_icon_ios.png",
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icons/kf_icon_android_fg.png",
        "backgroundImage": "./app/assets/icons/kf_icon_android_bg.png"
      },
      "icon": "./app/assets/icons/kf_icon_android.png"
    },
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "web": {
      "favicon": "./app/assets/kf_favicon.png"
    },
    "description": "Companion app for the Knife Fight board game. Board game and app both currently under development.",
    "githubUrl": "https://github.com/Dan-Carlin/KnifeFight"
  }
}
